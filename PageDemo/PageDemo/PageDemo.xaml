<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PageDemo.PageDemo">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand">
            <Button Text="Push" Clicked="PushPage"></Button>
            <Button Text="Pop"  Clicked="PopPage"></Button>
            <Button Text="PushModal"  Clicked="PushModalPage"></Button>
            <Button Text="PopModal"  Clicked="PopModalPage"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>